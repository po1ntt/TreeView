<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TreeView.Demos.TreeViewFolderFile.ObjectTree"
             Title="{Binding TitlePage}"
              Background="#111">


    <ScrollView>

        <VerticalStackLayout>
      
        <ScrollView  Orientation="Both"  VerticalScrollBarVisibility="Always">
            <CollectionView
                    Margin="10"
                    VerticalOptions="StartAndExpand"
                    ItemsSource="{Binding ItemsTree}"
                    ItemTemplate="{StaticResource TreeItemDataSelector}"
                    >
            </CollectionView>
        </ScrollView>

        </VerticalStackLayout>
    </ScrollView>
</ContentPage>